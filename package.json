{
	"name": "@harusame0616/result",
	"version": "0.1.2",
	"description": "Type-safe Result type for TypeScript",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"lint:check": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format:check": "biome format .",
		"format:fix": "biome format --write .",
		"type:check": "tsc --noEmit",
		"validate:check": "pnpm run lint:check && pnpm run format:check && pnpm run type:check",
		"validate:fix": "pnpm run lint:fix && pnpm run format:fix",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": [
		"result",
		"either",
		"error-handling",
		"typescript",
		"functional"
	],
	"author": "harusame0616",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/harusame0616/result.git"
	},
	"bugs": {
		"url": "https://github.com/harusame0616/result/issues"
	},
	"homepage": "https://github.com/harusame0616/result#readme",
	"packageManager": "pnpm@10.12.4",
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@tsconfig/strictest": "2.0.6",
		"@types/node": "24.7.2",
		"@vitest/ui": "3.2.4",
		"typescript": "5.9.3",
		"vitest": "3.2.4"
	}
}
